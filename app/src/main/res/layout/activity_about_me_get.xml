<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/relative_layout_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dimens_10dp"
            android:orientation="vertical" tools:ignore="UselessParent">

        <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dimens_10dp"
                android:layout_gravity="right" tools:ignore="RtlHardcoded">


            <ImageView
                    android:id="@+id/imageView_edit"
                    android:layout_width="@dimen/dimens_50dp"
                    android:layout_height="@dimen/dimens_50dp"
                    android:layout_weight="1"
                    android:layout_gravity="right"
                    android:visibility="visible"
                    app:srcCompat="@drawable/edit_my_services_and_offers_vendor"/>
        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimens_40dp"
                android:orientation="horizontal"
                android:weightSum="1">

            <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.35"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text="@string/text_first_name"
                    android:textColor="@color/yellow"
                    android:textSize="@dimen/text_size_18sp"
                    android:textStyle="bold"/>

            <TextView
                    android:id="@+id/textView_first_name_profile"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.05"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text=":"
                    android:textColor="@color/colorWhite"
                    android:textStyle="bold"
                    android:textSize="@dimen/text_size_18sp"/>

            <TextView
                    android:id="@+id/textView_full_name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.60"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text="@string/text_first_name"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/text_size_18sp"/>


        </LinearLayout>


        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimens_60dp"
                android:layout_marginTop="@dimen/dimens_5dp"
                android:orientation="horizontal">

            <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.35"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text="@string/text_email_id"
                    android:textColor="@color/yellow"
                    android:textSize="@dimen/text_size_18sp"
                    android:textStyle="bold"/>

            <TextView
                    android:id="@+id/textView_email_id_vendor"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.05"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text=":"
                    android:textStyle="bold"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/text_size_18sp"/>

            <TextView
                    android:id="@+id/textView_email_id"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.60"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text="@string/text_email_id"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/text_size_18sp"/>

        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimens_60dp"
                android:layout_marginTop="@dimen/dimens_5dp"
                android:orientation="horizontal">

            <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.35"
                    android:gravity="center_vertical"
                    android:layout_gravity="center"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text="@string/text_phone_number"
                    android:textColor="@color/yellow"
                    android:textSize="@dimen/text_size_18sp"
                    android:textStyle="bold"/>

            <TextView
                    android:id="@+id/textView_phone_number_vendor"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.05"
                    android:gravity="center_vertical"
                    android:layout_gravity="center"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text=":"
                    android:textStyle="bold"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/text_size_18sp"/>

            <TextView
                    android:id="@+id/textView_phone_number"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.60"
                    android:gravity="center_vertical"
                    android:layout_gravity="center"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text="@string/text_phone_number"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/text_size_18sp"/>

        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimens_40dp"
                android:layout_marginTop="@dimen/dimens_5dp"
            android:visibility="gone"
                android:orientation="horizontal">

            <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.35"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text="@string/text_date_of_birth"
                    android:textColor="@color/yellow"
                    android:textSize="@dimen/text_size_18sp"
                    android:textStyle="bold"/>

            <TextView
                    android:id="@+id/textView_dob_vendor"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.05"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text=":"
                    android:textColor="@color/colorWhite"
                    android:textStyle="bold"
                    android:textSize="@dimen/text_size_18sp"/>

            <TextView
                    android:id="@+id/textView_date_of_birth"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.60"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text="@string/text_first_name"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/text_size_18sp"/>


        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimens_40dp"
                android:layout_marginTop="@dimen/dimens_5dp"
                android:orientation="horizontal">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text="@string/text_about_me"
                    android:textColor="@color/yellow"
                    android:textSize="@dimen/text_size_18sp"
                    android:textStyle="bold"/>

            <TextView
                    android:id="@+id/textView_job_location"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dimens_10dp"
                    android:text=""
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/text_size_18sp"/>

        </LinearLayout>


        <TextView
                android:id="@+id/textView_about_me"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.60"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/dimens_10dp"
                android:text="@string/text_first_name"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/text_size_18sp"/>

    </LinearLayout>
</RelativeLayout>